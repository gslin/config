[alias]
    br = branch
    ci = commit -v
    co = checkout
    dc = diff --cached --word-diff=color
    di = diff --word-diff=color
    gl = log --decorate --graph --pretty=oneline
    glog = log --decorate --graph
    st = status -bs
[color]
    ui = auto
[core]
    preloadindex = yes
[merge]
    tool = vimdiff
[push]
    default = matching
